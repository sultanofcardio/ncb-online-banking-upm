{
    "name": "NCB Online Banking - Unblock Password Manager",
    "description": "Unblock your password manager on NCB's online banking login page",
    "version": "1.0.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://retail.ncbelink.com/*"
            ],
            "js": [
                "unblock.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "icons": {
        "16": "/img/icon-16.png",
        "32": "/img/icon-32.png",
        "48": "/img/icon-48.png",
        "128": "/img/icon-128.png"
    }
}